{
  "name": "curryfm",
  "version": "1.1.3",
  "description": "Curried Node wrapper around the public GET methods of Lastfm API",
  "main": "build.js",
  "scripts": {
    "test": "ava --verbose test.js",
    "build": "./node_modules/.bin/babel curryfm.js --out-file build.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "lastfm",
    "lastfmapi",
    "node",
    "lastfm",
    "curry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sidjain26/curryfm.git"
  },
  "author": "Siddharth Jain",
  "license": "ISC",
  "dependencies": {
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-babili": "0.1.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.16.3",
    "read-json-sync": "^2.0.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": {
      "babelrc": true
    }
  }
}
